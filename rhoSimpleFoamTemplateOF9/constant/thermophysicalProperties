/*    {*/
/*        molWeight   28.9;*/
/*    }*/
/*    thermodynamics*/
/*    {*/
/*        Cp          1005;*/
/*        Hf          0;*/
/*    }*/
/*    transport*/
/*    {*/
/*        mu          1.82e-05;*/
/*        Pr          0.71;*/
/*    }*/
/*}*/



//ARGON  

mixture
{
    specie
    {
        molWeight   $molWeight_val; //28.9
    }
    thermodynamics
    {
        Cp          $Cp_val;  //1005
        Hf          $Hf_val;
    }
    transport
    {
        mu          $mu_val;
        Pr          $Pr_val;   //0.71 air
    }
    
}



