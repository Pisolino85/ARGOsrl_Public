FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      thermophysicalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include        "include/constantsValues"

thermoType
{
    type            hePsiThermo;
    mixture         pureMixture;
    transport       const;   // valutare se farla in f(p) o f(T)
    thermo          hConst;
    equationOfState perfectGas;
    specie          specie;
    energy          sensibleEnthalpy;  //sensibleInternalEnergy
    
    
    
}

/*mixture*/
/*{*/
