FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      surfaceFeaturesDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //



inlet.stl
{
	extractionMethod    extractFromSurface;
	includedAngle       150;
	subsetFeatures
	{
		nonManifoldEdges       no;
		openEdges       yes;
	}
	writeObj            yes;
}

