FoamFile
{
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include        "include/initialConditions"

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform $turbulentKE;



boundaryField
{
    "wall.*"
    {
/*        type            kqRWallFunction;*/
/*        value           $internalField;*/

        // condizione LOW REYNOLDS
        type            kLowReWallFunction;
        value           $internalField;
    }

    inlet
    {
        type            turbulentIntensityKineticEnergyInlet;
        intensity       0.05;
        value           $internalField;
    }

    outlet
    {
/*        type            inletOutlet;*/
/*        inletValue      $internalField;*/
/*        value           $internalField;*/
        type            zeroGradient;
    }

    "symmet.*"
    {
        type            symmetry;
    }



}


