FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];
#include        "include/initialConditions"

internalField   uniform $turbulentEps;    

boundaryField
{
    "wall.*"
    {
        type            epsilonWallFunction;
        value           $internalField;
    }

    inlet
    {
        type            turbulentMixingLengthDissipationRateInlet;
        mixingLength    0.0105;  // 0.07*Dinlet
        value           $internalField;
    }

    "outlet.*"
    {
/*        type            inletOutlet;*/
/*        inletValue      $internalField;*/
/*        value           $internalField;*/

        type            zeroGradient;

    }

    "symmet.*"
    {
        type            symmetry;
    }
}


